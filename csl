[32m+[m[32mimport pygame[m
[32m+[m[32mpygame.init()[m
[32m+[m[32mfont = pygame.font.Font(None,30)[m
[32m+[m
[32m+[m[32mdef debug(info,y = 10, x = 10):[m
[32m+[m[32m    display_surface = pygame.display.get_surface()[m
[32m+[m[32m    debug_surf = font.render(str(info),True,'White')[m
[32m+[m[32m    debug_rect = debug_surf.get_rect(topleft= (x,y))[m
[32m+[m[32m    pygame.draw.rect(display_surface,'Black', debug_rect)[m
[32m+[m[32m    display_surface.blit(debug_surf, debug_rect)[m
