[32m+[m[32mimport pygame, sys[m
[32m+[m[32mfrom settings import *[m
[32m+[m
[32m+[m[32mclass Game:[m
[32m+[m[32m    def __init__(self):[m
[32m+[m
[32m+[m[32m        # main setup[m
[32m+[m[32m        pygame.init()[m
[32m+[m[32m        self.screen = pygame.display.set_mode((WIDTH, HEIGHT))[m
[32m+[m[32m        self.clock = pygame.time.Clock()[m
[32m+[m
[32m+[m[32m    def run(self):[m
[32m+[m[32m        while True:[m
[32m+[m[32m            for event in pygame.event.get():[m
[32m+[m[32m                if event.type == pygame.QUIT:[m
[32m+[m[32m                    pygame.quit()[m
[32m+[m[32m                    sys.exit()[m
[32m+[m
[32m+[m[32m            self.screen.fill('black')[m
[32m+[m[32m            pygame.display.update()[m
[32m+[m[32m            self.clock.tick(FPS)[m
[32m+[m
[32m+[m[32mif __name__ == '__main__':[m
[32m+[m[32m    game = Game()[m
[32m+[m[32m    game.run()[m
